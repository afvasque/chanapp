{% extends 'chanppEvImBundle::layout.html.twig' %}

{% block body -%}
    <h1>PlanEvaluacion list</h1>

    <table class="table table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>Objetivos</th>
                <th>Destinatarios</th>
                <th>Instanciasnumeroevaluaciones</th>
                <th>Instanciasplazo</th>
                <th>IdFichaProyecto</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('planevaluacion_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.objetivos }}</td>
                <td>{{ entity.destinatarios }}</td>
                <td>{{ entity.instanciasNumeroEvaluaciones }}</td>
                <td>{{ entity.instanciasPlazo }}</td>
                <td>{{ entity.idFichaProyecto }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('planevaluacion_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('planevaluacion_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('planevaluacion_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
