{% extends 'chanppEvImBundle::layout.html.twig' %}

{% block body -%}
    <h1>Editar Plan de Evaluación</h1>
 <ul class="record_actions">
    <li>
        <form action="{{ path('planevaluacion_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Eliminar</button>
        </form>
    </li>
</ul>
    <form action="{{ path('planevaluacion_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
        <input type="hidden" name="_method" value="PUT" />
        {{ form_widget(edit_form) }}
        <p>
            <button type="submit">Guardar Cambios</button>
        </p>
    </form>
     <ul class="record_actions">
    <li>
        <a href="{{ path('planevaluacion_show', { 'id': entity.id }) }}">
            Volver a Plan de Evaluación
        </a>
    </li>
       </ul>
{% endblock %}
