{% extends 'chanppEvImBundle::layout.html.twig' %}
{% block body -%}
    <h1>IndGestion list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Actions</th>
                <th>Variables obstaculo</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('indgestion_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.variablesobstaculo }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('indgestion_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('indgestion_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    {% endblock %}
