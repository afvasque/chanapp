{% extends 'chanppEvImBundle::layout.html.twig' %}

{% block body -%}
    <h1>PreguntaAlternativa list</h1>

    <table class="table table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>Enunciado</th>
                <th>Tipo</th>
                <th>Eje</th>
                <th>Numeropregunta</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('preguntaalternativa_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.enunciado }}</td>
                <td>{{ entity.tipo }}</td>
                <td>{{ entity.eje }}</td>
                <td>{{ entity.numeropregunta }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('preguntaalternativa_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('preguntaalternativa_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('preguntaalternativa_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
